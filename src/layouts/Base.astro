---
interface Props {
  title: string;
};

const { title } = Astro.props;

import Navbar from '@components/Navbar.astro';
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title} / Ramblings of Sean</title>
    <style>
      @font-face {
        font-family: "SF3Font";
        src: url("./fonts/SF3Font.ttf") format("truetype");
        font-display: swap;
      }
    </style>
  </head>

	<body>
    <Navbar></Navbar>
    <slot />
	</body>
</html>